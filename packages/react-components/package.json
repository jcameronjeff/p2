  "scripts": {
    "dev": "vite --config vite.web.config.js",
    "build": "tsc && vite build && npm run build_css",
    "build_css": "postcss lib/**/*.css --dir dist/style",
    "build_web": "vite build --config vite.web.config.js",
    "preview": "npm run build_web && vite preview",
    "storybook": "start-storybook -p 6006",
    "deploy": "gh-pages -d ../../docs -u \"github-actions-bot <support+actions@github.com>\"",
    "build-storybook": "build-storybook -o ../../docs"

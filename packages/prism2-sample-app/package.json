{
  "name": "@prism2/sample-react-app",
  "version": "2.0.0-beta.1",
  "files": [
    "dist",
    "src",
    "lib"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "test": "yarn build",
    "build": "tsc && vite build && yarn build:fonts",
    "build:fonts": "postcss lib/Styles/fonts.css -o dist/fonts.css",
    "postbuild": "tsc dist/index.es.js --declaration --emitDeclarationOnly --jsx react --esModuleInterop --allowJs --outDir dist",
    "preview": "vite preview",
    "precommit": "yarn test"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.5",
    "@prism2/tailwind-preset": "^1.0.0-alpha.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.7",
    "postcss": "^8.4.5",
    "postcss-apply": "^0.12.0",
    "postcss-cli": "^9.1.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^14.0.2",
    "postcss-minify": "^1.1.0",
    "postcss-nesting": "^10.1.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "tailwindcss": "^3.0.18",
    "ts-jest": "^27.1.3",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}

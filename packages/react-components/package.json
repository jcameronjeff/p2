{
  "name": "@prism2/react-components",
  "private": false,
  "version": "2.0.8-alpha.1",
  "files": [
    "dist",
    "dist/style.css",
    "lib"
  ],
  "style": "dist/style.css",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "storybook-deployer": {
    "gitUsername": "man-TeamPrism",
    "gitEmail": "man.TeamPrism@coxautoinc.com",
    "commitMessage": "chore: Deploy Storybook to GitHub Pages"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite --config vite.web.config.js",
    "test": "vitest --outputFile.json=unit-test-result.json --watch=false",
    "test_ui": "vitest --ui",
    "build": "vite build && npm run build_css",
    "build_css": "postcss lib/**/*.css --dir dist/style",
    "build_web": "vite build --config vite.web.config.js",
    "preview": "npm run build_web && vite preview",
    "docgen": "typedoc --out docs lib/index.ts lib/types.ts",
    "storybook": "start-storybook -p 6006 --ci",
    "deploy": "gh-pages -d ../../docs -u \"github-actions-bot <support+actions@github.com>\"",
    "build-storybook": "build-storybook -o ../../docs"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@prism2/icons": "^2.0.8-alpha.1",
    "@prism2/tailwind-elements": "^2.0.8-alpha.1",
    "@prism2/tailwind-preset": "^2.0.8-alpha.1",
    "@prism2/tailwind-theme": "^2.0.8-alpha.1",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "clsx": "^1.2.1",
    "cssnano": "^5.1.12",
    "postcss-apply": "^0.12.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^14.1.0",
    "postcss-minify": "^1.1.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "tailwindcss": "^3.1.4",
    "vite": "3",
    "vite-plugin-dts": "^1.2.1",
    "vitest": "^0.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-backgrounds": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",

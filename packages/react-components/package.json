{
  "name": "@prism2/react-components",
  "private": false,
  "version": "0.0.2",
  "files": [
    "dist",
    "dist/style",
    "dist/style.css"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite --config vite.web.config.js",
    "build": "tsc && vite build && yarn build:css",
    "build:css": "postcss lib/styles/*.css --dir dist/style",
    "build:web": "vite build --config vite.web.config.js",
    "preview": "yarn build:web && vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@headlessui/react": "^1.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@heroicons/react": "^1.0.6",
    "@prism2/tailwind-preset": "file:../tailwind-preset",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-interactions": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/addon-storysource": "^6.4.21",
    "@storybook/builder-vite": "^0.1.28",
    "@storybook/react": "^6.4.21",
    "@storybook/testing-library": "^0.0.9",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.4",
    "cssnano": "^5.1.5",
    "html-webpack-plugin": "^5.5.0",
    "postcss": "^8.4.5",
    "postcss-apply": "^0.12.0",
    "postcss-cli": "^9.1.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-minify": "^1.1.0",
    "postcss-nesting": "^10.1.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.9.1",
    "vite-plugin-dts": "^1.0.4"
  }
}
